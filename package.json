{
  "name": "bedrock-threads",
  "productName": "Bedrock Threads",
  "description": "A better playground for Amazon Bedrock.",
  "author": "Zachary Dodge",
  "private": true,
  "version": "0.0.0",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "tsc && vite build && cp main.js dist/main.js && cp package.json dist/package.json",
    "build:mac": "electron-vite build && electron-builder --mac",
    "dev": "electron-vite dev",
    "electron:dev": "npm run dev & electron .",
    "electron": "electron dist",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prebuild": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite dev"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.602.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
